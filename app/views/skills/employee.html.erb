<% provide(:title, 'Add Skill') %>
<h1>Skill:</h1>
<div class="row">
  <div class="span6 offset2">
    <%= form_tag(updateempskills_path) do %>
  <%#= label_tag(:x, "Task:") %>
  <%#= text_field_tag(:x )%>
  <%= label_tag(:x, "Skill:") %>
  <%#= text_field_tag(:x )%></br>
   <%= select_tag :x, options_for_select(@skills) %> </br>
  <%= label_tag(:y, "User:") %>
  <%#= text_field_tag(:y )%>
   <%= select_tag :y, options_for_select(@users) %> </br>
  <%= submit_tag "Continue", :class => "btn" %>
   <%end%>   

   <%#= link_to "Add task", menu_path, class: "btn btn-large btn-primary" %>

  </div>  </div>
</div>
