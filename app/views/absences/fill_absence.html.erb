<% provide(:title, 'Choose a record') %>
<h1>Choose a record</h1>
<div class="row">
  <div class="span6 offset2">
    <div style="white-space: nowrap">
  <%# @f.each do |l| %>
  <%#=l.date%></br>
    <%#=l.user%>
  <%#=link_to "See more", l%>
  <%#end%>
<%#@f = User.all%>

      <%#@f.each do |f|%>
